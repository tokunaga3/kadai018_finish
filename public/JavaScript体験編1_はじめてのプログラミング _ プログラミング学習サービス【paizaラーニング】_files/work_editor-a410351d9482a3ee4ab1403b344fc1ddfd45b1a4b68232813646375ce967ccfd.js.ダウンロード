(function(){var e,t,i;$(function(){return $("#code_zoom").click(function(){return t()}),$("#code_zoom_out").click(function(){return i()}),$(".theme_color_box").click(function(){return e($(this).data("theme"))})}),t=function(){var e,t;return 38<(t=(e=window.editor).getFontSize()+2)&&(t=38),e.setFontSize(t),$.cookie("editor_font_size",t,{expires:90})},i=function(){var e,t;return 6>(t=(e=window.editor).getFontSize()-2)&&(t=6),e.setFontSize(t),$.cookie("editor_font_size",t,{expires:90})},e=function(e){return window.editor.setTheme(get_theme_name(e)),$.cookie("editor_theme_color",e,{expires:90})},this.get_theme_name=function(e){var t;switch(t="ace/theme/solarized_light",e){case"dark":t="ace/theme/ambiance";break;case"white":t="ace/theme/chrome";break;case"orange":t="ace/theme/solarized_light"}return t},this.get_init_editor_theme=function(){var e;return e=$.cookie("editor_theme_color"),get_theme_name(e)},this.get_init_editor_font_size=function(){var e;return(6>(e=parseInt($.cookie("editor_font_size")))||38<e||isNaN(e))&&(e=12),$.cookie("editor_font_size",e,{expires:90}),e}}).call(this);